{
  "env": {
    "es6": true
  },
  "extends": [
    "airbnb",
    "plugin:react-native/all",
    "prettier",
    "prettier/react"
  ],
  "globals": {
    "__DEV__": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react-native", "jsx-a11y", "import", "react-hooks", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
